<!DOCTYPE html>
<html lang="en">
<head>
    <title>Space Labs Inc Storefront</title>
    <link rel="icon" type="image/x-icon" href="~/Content/images/spaceFavicon.png" />
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Mukta:300,400,700">
    <link rel="stylesheet" href="~/Content/fonts/icomoon/style.css">
    <link rel="stylesheet" href="//cdn.datatables.net/1.11.3/css/jquery.dataTables.min.css" />
    @Styles.Render("~/Content/css")
    <link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/themes/smoothness/jquery-ui.css">
</head>

<body>
    <div class="site-wrap">
        <header class="site-navbar" role="banner">
            <div class="site-navbar-top">
                <div class="container">
                    <div class="row align-items-center">

                        <div class="col-6 col-md-4 order-2 order-md-1 site-search-icon text-left">
                            <form action="@Url.Action("ProductSearch", "Filters")" class="site-block-top-search">
                                <span class="icon icon-search2"></span>
                                <input type="text" name="searchFilter" class="form-control border-0" placeholder="Search">
                            </form>
                        </div>

                        <div class="col-12 mb-3 mb-md-0 col-md-4 order-1 order-md-2 text-center">
                            <div class="site-logo">
                                <a href="@Url.Action("Index", "Home")" class="js-logo-clone">Space Labs Inc</a>
                            </div>
                        </div>

                        <div class="col-md-4 order-3 order-md-3 text-right">
                            <div class="site-top-icons">
                                <ul>
                                    <li class="nav-item dropdown">
                                        <a class="nav-link dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false"><span class="icon icon-person"></span></a>
                                        <div class="dropdown-menu account-dropdown">
                                            @Html.Partial("_LoginPartial")
                                        </div>
                                    </li>

                                    <a href="@Url.Action("Index", "ShoppingCart")" class="site-cart">
                                        <li class="icon icon-shopping_cart"></li>


                                        @if (Session["cart"] != null &&
                                            ((Dictionary<int, StoreFront2.Models.CartItemViewModel>)Session["cart"]).Count > 0
                                            )
                                        {
                                            <li class="count">
                                                @(((Dictionary<int, StoreFront2.Models.CartItemViewModel>)
                                                                                          Session["cart"]).Count)
                                            </li>
                                        }

                                    </a>

                                    <li class="icon icon-facebook-square p-1"></li>
                                    <li class="icon icon-twitter p-1"></li>
                                    <li class="icon icon-instagram p-1"></li>

                                </ul>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
            <div id="navblock" class="transparent-nav">
                <div class="container">
                    @*<button class="navbar-toggle">
                        <span></span>
                    </button>*@
                    <nav id="nav" class="site-navigation text-right text-md-center">
                        <ul class="site-menu js-clone-nav d-none d-md-block">
                            <li>@Html.ActionLink("Home", "Index", "Home")</li>
                            <li>@Html.ActionLink("About", "About", "Home")</li>
                            <li>@Html.ActionLink("Shop", "Index", "Products")</li>
                            <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                        </ul>
                    </nav>
                </div>
            </div>


        </header>

        <div>@RenderBody()</div>

        <footer class="site-footer border-top">
            <div class="container">
                <div class="row">
                    <div class="col-md-6 col-lg-4">
                        <div class="row">
                            <div class="col-md-12">
                                <h3 class="footer-heading mb-4">Navigations</h3>
                            </div>
                            <div class="col-md-12 col-lg-8">
                                <ul class="list-unstyled">
                                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                                    <li>@Html.ActionLink("About", "About", "Home")</li>
                                    <li>@Html.ActionLink("Shop", "Index", "Products")</li>
                                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12 col-lg-4">
                        <div class="block-5 mb-5">
                            <h3 class="footer-heading mb-4">Contact Info</h3>
                            <ul class="list-unstyled">
                                <li class="address">
                                    <p>
                                        203 SpaceLabs Rd. Launch Complex 25,<br />
                                        Cape Canaveral, Florida, USA
                                    </p>
                                </li>
                                <li class="phone"><a href="tel://23923929210">+2 392 3929 210</a></li>
                                <li class="email">emailaddress@domain.com</li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-12 col-lg-4">
                        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d66766.19056868591!2d-80.6116702924594!3d28.420293498254605!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x88e0a69b9e2aa171%3A0xa7ee0d38755c3679!2sLaunch%20Complex%2025!5e0!3m2!1sen!2sus!4v1643329069178!5m2!1sen!2sus" width="500" height="250" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
                    </div>

                </div>
                <div class="row mt-2 text-center">
                    <div class="col-md-12">
                        <p>
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                            Copyright &copy;
                            <script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script>
                            <script>document.write(new Date().getFullYear());</script> All rights reserved | This template is made by <a href="https://colorlib.com" target="_blank" class="text-primary">Colorlib</a> and customized by Eric Faith!<br />
                            This site is for educational purposes only!
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                        </p>
                    </div>

                </div>
            </div>
        </footer>
    </div>

    @Scripts.Render("~/bundles/bootstrap")
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script src="https://cdn.jsdelivr.net/jquery.ajax.unobtrusive/3.2.4/jquery.unobtrusive-ajax.min.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>