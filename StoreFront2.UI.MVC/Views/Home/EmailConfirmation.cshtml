@model StoreFront2.Models.ContactViewModel

@{
    ViewBag.Title = "Email Confirmation";
}

<div class="text-center m-2">
    <h2>@ViewBag.Title</h2>
    <h4>We have received the following message:</h4>
    <p>@Model.Message</p>
    <h4>We will respond as soon as we can to @Model.Email.  Thank you for contacting us!</h4>
</div>

